<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <!--<link rel="stylesheet" href="/css/index.css">-->
    <!--<script src="/js/index1.js"></script>-->
</head>
<style>
    .add{
        text-align: center;
    }
</style>
<body>
    <table border="1" cellspacing="0" cellpadding="0" width="500px" height="200px" align="center">
        <tr>
            <td>姓名</td>
            <td>性别 </td>
            <td>年龄</td>
            <td>编辑</td>
        </tr>
        <%if(result.length==0){%>
        <tr>无数据输入</tr>
        <%}else{
            for(var i=0;i<=result.length-1;i++){
        %>
        <tr>
            <td>
                <%=result[i].name%>
            </td>
            <td>
                <%=result[i].sex%>
            </td>
            <td>
                <%=result[i].age%>
            </td>
            <td>
                <a href="/edit?id=<%=result[i].id%>">编辑</a>
                <a href="/del?id=<%=result[i].id%>">删除</a>
            </td>
        </tr>
        <%}}%>
        <tr>
            <td colspan="4" class="add"><a href="/add">添加</a></td>
        </tr>
    </table>
</body>
</html>